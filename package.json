{
  "name": "stream-scene",
  "version": "1.0.0",
  "type": "module",
  "main": "server/app.ts",
  "ts-node": {
    "esm": true,
    "compilerOptions": {
      "module": "ES2022",
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "esModuleInterop": true
    }
  },
  "scripts": {
    "analyze": "webpack --config webpack.config.mjs --mode development --profile --json > stats.json && npx webpack-bundle-analyzer stats.json",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "dev:client": "webpack --config webpack.config.mjs --mode development --watch",
    "dev:server": "NODE_ENV=development nodemon --watch server --watch client --ext ts,tsx,js,jsx --exec \"tsx server/app.ts\"",
    "dev:debug": "NODE_ENV=development DEBUG=* nodemon --watch server --ext ts --exec \"tsx server/app.ts\"",
    "build": "npm run build:client:prod && npm run build:server",
    "build:client": "webpack --config webpack.config.mjs --mode development",
    "build:client:prod": "webpack --config webpack.config.mjs --mode production",
    "build:server": "tsc -p server/tsconfig.json",
    "start": "NODE_ENV=production node dist/server/app.js",
    "start:dev": "NODE_ENV=development tsx server/app.ts",
    "start:prod": "NODE_ENV=production tsx server/app.ts",
    "db:migrate": "sequelize-cli db:migrate",
    "db:seed": "sequelize-cli db:seed:all",
    "seed": "tsx server/db/seed.ts",
    "seed:force": "tsx server/db/seed.ts --force",
    "seed:production": "NODE_ENV=production tsx server/db/seed.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.864.0",
    "@aws-sdk/client-transcribe": "^3.866.0",
    "@aws-sdk/s3-request-presigner": "^3.864.0",
    "@google-cloud/vision": "^5.3.3",
    "@google/generative-ai": "^0.24.1",
    "@types/socket.io": "^3.0.1",
    "aws-sdk": "^2.1692.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^4.21.2",
    "express-session": "^1.18.2",
    "fluent-ffmpeg": "^2.1.3",
    "framer-motion": "^12.23.12",
    "lightningcss": "^1.30.1",
    "lucide-react": "^0.544.0",
    "mime-types": "^3.0.1",
    "multer": "^2.0.2",
    "mysql2": "^3.14.2",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-twitter": "^1.0.4",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hot-toast": "^2.5.2",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.8.0",
    "react-sketch-canvas": "^6.2.0",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tesseract.js": "^6.0.1",
    "twitter-api-v2": "^1.24.0",
    "wavesurfer.js": "^7.10.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.35.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.23",
    "@types/express-session": "^1.18.2",
    "@types/mime-types": "^3.0.1",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.9.1",
    "@types/node-cron": "^3.0.11",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@types/react-router-dom": "^5.3.3",
    "@types/sequelize": "^4.28.20",
    "@types/wavesurfer.js": "^6.0.12",
    "autoprefixer": "^10.4.21",
    "buffer": "^6.0.3",
    "concurrently": "^9.2.1",
    "crypto-browserify": "^3.12.1",
    "css-loader": "^6.8.1",
    "dotenv-webpack": "^8.1.1",
    "eslint": "^9.35.0",
    "html-webpack-plugin": "^5.6.3",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "process": "^0.11.10",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.4",
    "tailwindcss": "^4.1.11",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.3",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.44.0",
    "util": "^0.12.5",
    "vm-browserify": "^1.1.2",
    "webpack": "^5.100.2",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2"
  }
}
